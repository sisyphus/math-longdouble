#include <stdio.h>
#include <math.h>
#include <quadmath.h>

int main(void) {
  long double check, sld, ld1 = 10.0L, ld2 = -298.0L;
  sld = strtold("1e-298", NULL);
  check = powl(ld1, ld2);
  if(check == sld) printf("%.21Lg\n", check);
  else printf("miscalculated to %.21Lg\n", check);
  return 0;
}

